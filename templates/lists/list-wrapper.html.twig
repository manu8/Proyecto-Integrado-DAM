{% extends 'layout.html.twig' %}

{% block content %}
  {% if studies_list is defined %}
    {% include 'lists/studies.html.twig' %}
  {% elseif knowledges_list is defined %}
    {% include 'lists/knowledges.html.twig' %}
  {% else %}
    {% include 'lists/companies.html.twig' %}
{% endblock %}

{% block scripts %}
  {{ parent() }}
  {% if studies_list is defined %}
    <script src="{{ asset('js/studies-lists.js') }}"></script>
  {% elseif knowledges_list is defined %}
    <script src="{{ asset('js/knowledges-list.js') }}"></script>
  {% else %}
    <script src="{{ asset('js/companies-list.js') }}"></script>
  {% endif %}
{% endblock %}